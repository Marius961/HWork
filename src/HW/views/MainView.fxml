<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="300.0" prefWidth="530.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="HW.controllers.MainViewController">
   <children>
      <TableView fx:id="subjTable" layoutX="58.0" layoutY="85.0" prefHeight="156.0" prefWidth="440.0">
        <columns>
          <TableColumn fx:id="colSubject" prefWidth="244.0" text="Subject" />
          <TableColumn fx:id="colLecture" prefWidth="178.0" text="Lecture" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <HBox layoutX="371.0" layoutY="243.0" spacing="5.0">
         <children>
            <Button mnemonicParsing="false" onAction="#addButtonClickMethod" prefHeight="25.0" prefWidth="39.0" text="Add" />
            <Button mnemonicParsing="false" onAction="#delButtonClickMethod" prefHeight="25.0" prefWidth="39.0" text="Del" />
            <Button maxWidth="79.0" mnemonicParsing="false" onAction="#editButtonClickMethod" prefHeight="10.0" prefWidth="39.0" text="Edit" />
         </children>
      </HBox>
      <Button fx:id="topButton" layoutX="58.0" layoutY="55.0" mnemonicParsing="false" onAction="#top" prefHeight="25.0" prefWidth="408.0" text="Вверх" />
      <Button fx:id="bottomButton" layoutX="58.0" layoutY="243.0" mnemonicParsing="false" onAction="#down" prefHeight="25.0" prefWidth="308.0" text="Вниз" />
      <Button layoutX="8.0" layoutY="15.0" mnemonicParsing="false" onAction="#refresh" text="Button" />
       <Label fx:id="dayName" alignment="CENTER" contentDisplay="CENTER" layoutX="252.0" layoutY="27.0" text="Day">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <VBox layoutX="18.0" layoutY="113.0" spacing="10.0">
         <children>
            <Label fx:id="firstLessonTime" text="Time1" />
            <Label fx:id="secondLessonTime" text="Time2" />
            <Label fx:id="thirdLessonTime" text="Time3" />
            <Label fx:id="fourthLessonTime" text="Time4" />
            <Label fx:id="fifthLessonTime" text="Time5" />
         </children>
      </VBox>
      <Button cache="true" graphicTextGap="0.0" layoutX="471.0" layoutY="55.0" mnemonicParsing="false" text="⚙" />
   </children>
</AnchorPane>
