<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="450.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="HW.controllers.MainViewController">
   <children>
      <TableView fx:id="subjTable" layoutX="50.0" prefHeight="450.0" prefWidth="550.0">
        <columns>
            <TableColumn fx:id="colTime" prefWidth="95.0" text="Time" />
          <TableColumn fx:id="colSubject" prefWidth="250.0" text="Subject" />
          <TableColumn fx:id="colLecture" prefWidth="200.0" text="Lecture" />
        </columns>
      </TableView>
      <Button mnemonicParsing="false" onAction="#clickonMn" prefHeight="50.0" prefWidth="50.0" text="Mo" />
      <Button layoutY="50.0" minHeight="50.0" mnemonicParsing="false" onAction="#clickonTu" prefWidth="50.0" text="Tu" />
      <Button layoutY="100.0" mnemonicParsing="false" onAction="#clickonWe" prefHeight="50.0" prefWidth="50.0" text="We" />
      <Button layoutY="150.0" mnemonicParsing="false" onAction="#clickonTh" prefHeight="50.0" prefWidth="50.0" text="Th" />
      <Button layoutY="200.0" mnemonicParsing="false" onAction="#clickonFr" prefHeight="50.0" prefWidth="50.0" text="Fr" />
      <Button layoutY="400.0" maxWidth="50.0" mnemonicParsing="false" onAction="#SetButtonClickMethod" prefHeight="50.0" text="Set" />
      <Button layoutY="350.0" maxWidth="50.0" mnemonicParsing="false" onAction="#InsButtonClickMethod" prefHeight="50.0" text="Ins" />
      <Button layoutY="300.0" mnemonicParsing="false" onAction="#DelButtonClickMethod" prefHeight="50.0" prefWidth="50.0" text="Del" />
      <Button layoutY="250.0" mnemonicParsing="false" onAction="#AddButtonClickMethod" prefHeight="50.0" prefWidth="50.0" text="Add" />
   </children>
</AnchorPane>
